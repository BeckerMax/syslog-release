hostname: <%= "#{name}-#{index}" %>

<% if_p("syslog.blackbox.destination.address") do %>
syslog:
  destination:
    transport: <%= p("syslog.blackbox.destination.transport") %>
    address: <%= p("syslog.blackbox.destination.address") %>

  source_dir: <%= p("syslog.blackbox.source_dir") %>
<% end %>
